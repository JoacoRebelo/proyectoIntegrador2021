<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link width="" rel="shortcut icon" href="/images/loguitodh.png" />
	<title> Super DH</title>
	<!-- Font Awesome Stylesheet -->
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.11.2/css/all.min.css">
	<link rel="preconnect" href="https://fonts.gstatic.com">
	<link href="https://fonts.googleapis.com/css2?family=Quicksand&display=swap" rel="stylesheet">
	<link href="https://fonts.googleapis.com/css2?family=Londrina+Solid:wght@300&display=swap" rel="stylesheet">
	<link href="https://fonts.googleapis.com/css2?family=Comfortaa:wght@500&family=M+PLUS+Rounded+1c&family=New+Tegomin&display=swap" rel="stylesheet">
	<link href="https://fonts.googleapis.com/css2?family=M+PLUS+Rounded+1c&family=Spartan:wght@500&display=swap" rel="stylesheet">
	<!-- Bootstrap Grid Stylesheet -->
	<link rel="stylesheet" href="/stylesheets/bootstrap-grid.min.css">
	<!-- Own Stylesheet -->
	<link rel="stylesheet" href="/stylesheets/app.css">
</head>

<header class="main-header shadow-lg rounded" id="Principal">
	<div class="container">
		<div class="row align-items-center">
			<div class="col-5 col-md-2">
				<a href="/" class="main-header_home-link">
					<img class="logoPrincipal" src="../images/superdh.png">
				</a>
			</div>
			<div class="col-7 col-md-6">
				<form action="/search-results" method="GET" class="search-form">
					<input type="text" name="search" placeholder="Buscar productos, marcas y más"
						class="search-form_input">
						<button type="submit" class="search-form_button">
							<i class="fas fa-search">
								<input type="submit" value="" class="botonInput">
							</i>
						</button>
				</form>
			</div>
			<ul class="right-navbar" style="align-items: right;">
				<% if(! usuarioLogueado) { %>
					<li>
						<a href="/product-add"> Subir Producto <i class="fas fa-upload"></i>
						</a>
					</li>
				    <li>
					   <a href="/profile/<%= idUsuario %>"><%= nombreUsuario %> <i class="fas fa-user"></i>
					   </a>
				    </li>
				    <li>
					   	<a href="/logout"> Cerrar sesion <i class="fas fa-sign-in-alt"></i>
						</a>
				    </li>
				   
			   <% } else { %>
				   <li>
					   <a href="/register">Creá tu cuenta <i class="far fa-address-card" ></i>
					   </a>
				   </li>
				   <li>
					   <a href="/login">Ingresá <i class="fas fa-sign-in-alt"></i></a>
				   </li> 
			   <% } %>
		   </ul>
		</div>
		
	</div>
</header>